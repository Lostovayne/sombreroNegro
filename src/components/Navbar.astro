---
import { Menu } from "@/constants"
---

<nav class="w-full py-8 hidden lg:block shadow-md" >
  <ul class="flex items-center w-full justify-evenly gap-5  font-semibold max-w-screen-2xl mx-auto ">
    {Menu.map((item) => <li><a href={item.url} class="text-decoration-none capitalize text-base text-gray-500 hover:text-gray-600 transition-colors" >{item.name}</a></li>)}
  </ul>
</nav>